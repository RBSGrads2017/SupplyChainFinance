    <script type="text/javascript">
    	var app = angular.module("app", [  ]);
		app.controller('CtrlChecklist', function($scope) {
              $scope.categories = {
                "Electronics":false,
                "Appliances":false,
                "Clothings":false
              }
			  $scope.products = {
					"Electronics":{
						"Camera":false,
						"Mobile":false
					},
					"Appliances":{
						"Refrigerator":false,
						"Microwave":false
					},
					"Clothings":{
						"Shirts":false,
						"Pants":false
					}
			 }
			
			$scope.arrayCateg=[];
			$scope.arraySub=[];
			$scope.temp=[];
			$scope.categoryToggle = function(category){
				for(item in $scope.products[category])
				{
					$scope.products[category][item] = $scope.categories[category];
				}
			}
			$scope.subcategoryToggle = function(category, product) {

				var flag = true;
				if ($scope.categories[category] == true) {
					
					//this is if no sub category selected then main category gets disselected.
					for(item in $scope.products[category])
					{	
						if ($scope.products[category][item] == true) {
							flag = false;
							break;
						}
					}

					if (flag == true) {
						console.log("hey")
						$scope.categories[category] = false;
					}

				} else if (($scope.products[category][product] == true) && ($scope.categories[category] == false)) {
					$scope.categories[category] = true;
				}
			}

			$scope.addButton = function() {
				$scope.arrayCateg=[];
				for (categ in $scope.categories) {				
					if ($scope.categories[categ] == true) {
						$scope.arrayCateg.push(categ);
						$scope.arrayCateg[categ] = [];
						for (item in $scope.products[categ]) {
							if ($scope.products[categ][item] == true) {
								$scope.arrayCateg[categ].push(item);
							}
						}
					}
				}
				
			}

			$scope.addAllButton = function() {
				$scope.arrayCateg=[];
				for (categ in $scope.categories) {
                    $scope.categories[categ] = true;				
					$scope.arrayCateg.push(categ);
					$scope.arrayCateg[categ] = [];
					for (item in $scope.products[categ]) {
                        $scope.products[categ][item] = true;
						$scope.arrayCateg[categ].push(item);
					}
				}
				console.log($scope.arrayCateg);
			}
            $scope.removeAllButton = function(){
                $scope.arrayCateg=[];
                for (categ in $scope.categories){
                    $scope.categories[categ] = false;
                    for (item in $scope.products[categ]){
                        $scope.products[categ][item] = false;
                    }
                }
            }

		});
    </script>>
